#初始化zshrc配置文件
setup_zshrc(){
	cd ${RES}/zsh
	cat zshrc > ~/.zshrc
	cat alia >> ~/.zshrc
	setup_env
	echo -e '\n#[Environment]\nsource env' >> ~/.zshrc
	echo "setup .zshrc success"
}